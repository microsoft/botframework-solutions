{
    "name": "assistant",
    "version": "1.0.0",
    "description": "",
    "author": "Microsoft Bot Framework Team",
    "license": "MIT",
    "main": "lib/index.js",
    "scripts": {
        "clean": "rimraf ./lib",
        "copy-templates": "copyfiles --up 1 \"./src/**/*.json\" \"./lib\"",
        "prebuild": "npm run lint",
        "build": "tsc --p tsconfig.json && npm run copy-templates",
        "post-install": "npm run build && node ./deploymentScripts/webConfigPrep.js",
        "lint": "tslint -t vso ./src/**/*.ts",
        "start": "npm run build && node ./lib/index.js NODE_ENV=development",
        "watch": "nodemon ./lib/index.js NODE_ENV=development",
        "test": "mocha ./test/"
    },
    "dependencies": {
        "@microsoft/microsoft-graph-client": "^1.3.0",
        "@microsoft/microsoft-graph-types": "^1.5.0",
        "applicationinsights": "^1.0.4",
        "azure-cognitiveservices-contentmoderator": "^4.0.0",
        "bot-solution": "^1.0.0",
        "botbuilder": "^4.2.1",
        "botbuilder-ai": "^4.2.1",
        "botbuilder-azure": "^4.2.1",
        "botbuilder-core": "^4.2.1",
        "botbuilder-dialogs": "^4.2.1",
        "botframework-config": "^4.2.1",
        "botframework-schema": "^4.2.1",
        "dotenv": "^6.0.0",
        "i18n": "^0.8.3",
        "ms-rest-azure": "^2.5.0",
        "restify": "^7.2.1"
    },
    "devDependencies": {
        "copyfiles": "^2.1.0",
        "nodemon": "^1.18.4",
        "replace": "^1.0.0",
        "rimraf": "^2.6.2",
        "typescript": "^3.2.2",
        "mocha": "^5.2.0",
        "tslint": "^5.12.1"
    },
    "env": {
        "mocha": true,
        "node": true
    }
}
