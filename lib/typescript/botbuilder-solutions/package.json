        "@typescript-eslint/parser": "^1.10.2",
        "copyfiles": "^2.1.0",
        "eslint": "^5.16.0",
        "mocha": "^6.1.4",
        "mocha-junit-reporter": "^1.22.0",
        "nyc": "^14.1.1",
        "replace": "^1.0.0",
        "rimraf": "^2.6.2",
        "tslint": "^5.12.1",
        "tslint-microsoft-contrib": "^6.2.0",
        "typescript": "3.4.5"
    },
    "env": {
        "mocha": true,
        "node": true
    }
