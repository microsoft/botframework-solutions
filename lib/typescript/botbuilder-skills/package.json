    "homepage": "https://github.com/Microsoft/AI/",
    "author": "Microsoft",
    "license": "MIT",
    "main": "lib/index.js",
    "scripts": {
        "clean": "rimraf ./lib",
        "copy-templates": "copyfiles --up 1 \"./src/**/*.json\" \"./lib\"",
