{
  "type": "AdaptiveCard",
  "id": "MenuCard",
  "body": [
    {
      "type": "Container",
      "backgroundImage": "data:image/png;base64,",
      "items": [
        {
          "type": "ColumnSet",
          "columns": [
            {
              "type": "Column",
              "items": [
                {
                  "type": "Image",
                  "id": "icon",
                  "horizontalAlignment": "Center",
                  "url": "data:image/svg+xml;base64,",
                  "size": "Small",
                  "width": "30px",
                  "height": "30px"
                }
              ],
              "width": "auto"
            },
            {
              "type": "Column",
              "verticalContentAlignment": "Center",
              "items": [
                {
                  "type": "TextBlock",
                  "id": "title",
                  "size": "Large",
                  "weight": "Bolder",
                  "color": "Light",
                  "text": "{Type} Menu"
                }
              ],
              "width": "stretch"
            }
          ]
        },
        {
          "type": "TextBlock",
          "text": "{TimeAvailable}",
          "weight": "Bolder",
          "color": "Light"
        }
      ]
    },
    {
      "type": "Container",
      "id": "items",
      "items": []
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.0",
  "speak": "{Speak}"
}