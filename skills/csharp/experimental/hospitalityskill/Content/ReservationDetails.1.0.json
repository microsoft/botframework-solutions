{
  "type": "AdaptiveCard",
  "id": "ReservationDetails",
  "body": [
    {
      "type": "Container",
      "items": [
        {
          "type": "TextBlock",
          "id": "title",
          "size": "Large",
          "weight": "Bolder",
          "color": "Dark",
          "text": "{Title}"
        }
      ],
      "width": "stretch"
    },
    {
      "type": "Container",
      "id": "items",
      "items": [
        {
          "type": "FactSet",
          "facts": [
            {
              "title": "Check-in Date",
              "value": "{CheckInDate}"
            },
            {
              "title": "Check-out Date",
              "value": "{CheckOutDate}"
            },
            {
              "title": "Check-out Time",
              "value": "{CheckOutTime}"
            }
          ]
        }
      ]
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.0",
  "speak": "{Speak}"
}