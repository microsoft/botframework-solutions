{
  "type": "AdaptiveCard",
  "id": "MenuCard",
  "body": [
    {
      "type": "Container",
      "items": [
        {
          "type": "ColumnSet",
          "columns": [
            {
              "type": "Column",
              "verticalContentAlignment": "Center",
              "items": [
                {
                  "type": "TextBlock",
                  "id": "title",
                  "size": "Large",
                  "weight": "Bolder",
                  "color": "Dark",
                  "text": "{Type} Menu"
                }
              ],
              "width": "stretch"
            }
          ]
        },
        {
          "type": "TextBlock",
          "text": "{TimeAvailable}",
          "weight": "Bolder",
          "color": "Dark"
        }
      ]
    },
    {
      "type": "Container",
      "id": "items",
      "items": []
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.0",
  "speak": "{Speak}"
}