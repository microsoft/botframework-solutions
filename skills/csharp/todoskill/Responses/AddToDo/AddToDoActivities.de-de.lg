    Text = @{AddMoreTask.Text(ListType)}
    Speak = @{AddMoreTask.Text(ListType)}
    InputHint = expectingInput
]

# AddMoreTaskConfirmFailed
[Activity
    Text = @{AddMoreTaskConfirmFailed.Text(ListType)}
    Speak = @{AddMoreTaskConfirmFailed.Text(ListType)}
    InputHint = expectingInput
]

# AskAddDupTaskPrompt
