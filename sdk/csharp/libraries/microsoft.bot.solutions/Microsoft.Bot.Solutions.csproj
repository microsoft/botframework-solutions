<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version Condition=" '$(PackageVersion)' == '' ">4.0.0-local</Version>
    <Version Condition=" '$(PackageVersion)' != '' ">$(PackageVersion)</Version>
    <PackageVersion Condition=" '$(PackageVersion)' == '' ">4.0.0-local</PackageVersion>
    <PackageVersion Condition=" '$(PackageVersion)' != '' ">$(PackageVersion)</PackageVersion>
    <Configurations>Debug;Release;Documentation;Debug - NuGet Packages;</Configurations>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Microsoft.Bot.Solutions</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>Microsoft</Company>
    <Authors>Microsoft</Authors>
    <Description>Shared library for Conversational AI Virtual Assistants and Skills.</Description>
  </PropertyGroup>
  
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <SignAssembly>true</SignAssembly>
    <DelaySign>true</DelaySign>
    <AssemblyOriginatorKeyFile>..\..\build\35MSSharedLib1024.snk</AssemblyOriginatorKeyFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  
  <ItemGroup>
    <None Remove="Authentication\AuthenticationActivities.de-de.lg" />
    <None Remove="Authentication\AuthenticationActivities.es-es.lg" />
    <None Remove="Authentication\AuthenticationActivities.fr-fr.lg" />
    <None Remove="Authentication\AuthenticationActivities.it-it.lg" />
    <None Remove="Authentication\AuthenticationActivities.lg" />
    <None Remove="Authentication\AuthenticationActivities.zh-cn.lg" />
  </ItemGroup>
  
  <ItemGroup>
    <Content Include="Authentication\AuthenticationActivities.de-de.lg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Authentication\AuthenticationActivities.es-es.lg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Authentication\AuthenticationActivities.fr-fr.lg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Authentication\AuthenticationActivities.it-it.lg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Authentication\AuthenticationActivities.lg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Authentication\AuthenticationActivities.zh-cn.lg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="AdaptiveCards" Version="1.2.3" />
    <PackageReference Include="Microsoft.ApplicationInsights" Version="2.8.1" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.2" />
    <PackageReference Include="Microsoft.Azure.CognitiveServices.ContentModerator" Version="2.0.0" />
    <PackageReference Include="Microsoft.Bot.Builder.AI.Luis" Version="4.8.1" />
    <PackageReference Include="Microsoft.Bot.Builder.AI.QnA" Version="4.8.1" />
    <PackageReference Include="Microsoft.Bot.Builder.Azure" Version="4.8.1" />
    <PackageReference Include="Microsoft.Bot.Builder.Dialogs" Version="4.8.1" />
    <PackageReference Include="Microsoft.Bot.Builder.Integration.AspNet.Core" Version="4.8.1" />
    <PackageReference Include="Microsoft.Bot.Builder.LanguageGeneration" Version="4.8.1-rc0" />
    <PackageReference Include="Microsoft.Bot.Builder.TemplateManager" Version="4.8.1" />
    <PackageReference Include="Microsoft.Bot.Configuration" Version="4.8.1" />
    <PackageReference Include="Microsoft.Bot.Schema" Version="4.8.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="2.1.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="2.1.0" />
    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="2.2.0" />
    <PackageReference Include="NCrontab" Version="3.3.1" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="AsyncUsageAnalyzers" Version="1.0.0-alpha003" PrivateAssets="all" />
    <PackageReference Include="StyleCop.Analyzers" Version="1.1.0-beta008" PrivateAssets="all" />
    <PackageReference Include="SourceLink.Create.CommandLine" Version="2.8.3" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Feedback\FeedbackResponses.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>FeedbackResponses.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\CommonStrings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>CommonStrings.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Feedback\FeedbackResponses.de.resx">
      <Generator></Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Feedback\FeedbackResponses.es.resx">
      <Generator></Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Feedback\FeedbackResponses.fr.resx">
      <Generator></Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Feedback\FeedbackResponses.it.resx">
      <Generator></Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Feedback\FeedbackResponses.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>FeedbackResponses.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\CommonStrings.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>CommonStrings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  
</Project>
