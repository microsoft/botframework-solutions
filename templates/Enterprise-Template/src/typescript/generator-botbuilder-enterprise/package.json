{
  "name": "generator-botbuilder-enterprise",
  "version": "1.0.0",
  "description": "Project template for advanced bot scenarios using the Bot Builder SDK V4.",
  "homepage": "https://github.com/Microsoft/AI/tree/master/templates/Enterprise-Template/src/typescript",
  "author": {
    "name": "Microsoft",
    "email": "botframework@microsoft.com",
    "url": "http://dev.botframework.com"
  },
  "files": [
    "generators",
    "app",
    "dialog"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "botbuilder",
    "bots",
    "bot framework",
    "Microsoft AI",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.9.1",
    "yeoman-assert": "^3.1.1",
    "eslint": "^5.12.0",
    "prettier": "^1.15.3",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-config-xo": "^0.26.0",
    "mocha": "^5.2.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "lodash": "^4.17.11",
    "yeoman-generator": "^3.2.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "mocha": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "cd test/ && mocha --opts mocha.opts"
  },
  "repository": "https://github.com/southworkscom/AI",
  "license": "MIT"
}
