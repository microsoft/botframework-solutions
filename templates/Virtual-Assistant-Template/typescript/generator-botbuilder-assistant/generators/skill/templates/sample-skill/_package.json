{
    "name": "<%= skillName %>",
    "version": "1.0.0",
    "description": "<%= skillDescription %>",
    "author": "",
    "license": "MIT",
    "main": "lib/index.js",
    "scripts": {
        "clean": "rimraf ./lib",
        "copy-templates": "copyfiles --up 1 \"./src/**/*.json\" \"./lib\"",
        "prebuild": "npm run lint",
        "build": "tsc --p tsconfig.json && npm run copy-templates",
        "lint": "eslint ./src/**/*.ts",
        "lint-fix": "eslint --fix ./src/**/*.ts",
        "start": "npm run build && node ./lib/index.js NODE_ENV=development",
        "watch": "nodemon ./lib/index.js NODE_ENV=development",
        "test": "mocha",
        "coverage": "nyc mocha",
        "test-coverage-ci": "nyc --reporter=cobertura mocha --reporter mocha-junit-reporter"
    },
    "dependencies": {
        "botbuilder": "^4.5.3",
        "botbuilder-ai": "^4.5.3",
        "botbuilder-applicationinsights": "^4.5.3",
        "botbuilder-azure": "^4.5.3",
        "botbuilder-dialogs": "^4.5.3",
        "botbuilder-skills": "^4.4.8",
        "botbuilder-solutions": "^4.4.8",
        "botframework-config": "^4.5.3",
        "botframework-connector": "^4.5.3",
        "botframework-schema": "^4.5.3",
        "dotenv": "^6.0.0",
        "i18next": "^15.0.6",
        "i18next-node-fs-backend": "^2.1.1",
        "restify": "^7.2.1"
    },
    "devDependencies": {
        "@types/i18next-node-fs-backend": "^0.0.30",
        "@types/node": "^10.10.1",
        "@types/restify": "^7.2.4",
        "@typescript-eslint/eslint-plugin": "^2.1.0",
        "@typescript-eslint/eslint-plugin-tslint": "^2.1.0",
        "@typescript-eslint/parser": "^2.1.0",
        "copyfiles": "^2.1.0",
        "eslint": "^5.16.0",
        "nock": "^10.0.6",
        "mocha": "^6.1.4",
        "mocha-junit-reporter": "^1.22.0",
        "nodemon": "^1.18.4",
        "nyc": "^14.1.1",
        "replace": "^1.0.0",
        "rimraf": "^2.6.2",
        "tslint": "^5.12.1",
        "tslint-microsoft-contrib": "^6.0.0",
        "typescript": "^3.2.2"
    },
    "env": {
        "mocha": true,
        "node": true
    }
}
